.upload1{display:block; width:6.5625rem;height: 0.5rem;position: relative; text-decoration: none;background:url(../images/icon008.jpg) right center no-repeat;background-size:0.3125rem;margin-top:0.46875rem;height: 0.5rem;line-height:0.5rem;color:#666;text-align:right;padding-right:0.54688rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;font-size:0.375rem;}
.change1{position: absolute; overflow: hidden; right: 0;top: 0;opacity: 0;width:6.5625rem;}



/* 房主-首页 */  
