@charset "UTF-8";
/* CSS Document */
* {
  margin: 0;
  padding: 0; }

a {
  text-decoration: none; }

li {
  list-style: none; }

img {
  border: none;
  display:block
   }

input {
  outline: none;
  border: none;
  font-family: "Microsoft YaHei";
  -webkit-appearance: none; border-radius:0}

body {
  font-family: "Microsoft YaHei";
  font-size: 0.375rem;
  color: #666;
  width: 10rem;
  background:#F0F0F0
  }

/* 代理商管理-首页 */  
.agent{padding-bottom:1.5625rem;}
.agent-top{width:10rem;height:4.59375rem;background:url(../images/bg1.jpg) no-repeat;background-size:10rem;padding-top:0.3125rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;position:relative;overflow:hidden;width:100%;background-size:cover}

.agent-top .headline h1{width:3.90625rem;font-size:0.5625rem;color:#fff;position:absolute;left:0;right:0;margin:0 auto;text-align:center} 
.agent-top .headline .quit{float:right;font-size:0.375rem;color:#fff;margin-right:0.3125rem;display:inline;margin-top:0.15625rem;}
.agent-top .headline .quit i{display:inline-block;width:0.39063rem;height:0.375rem;background:url(../images/quit.png) left center no-repeat;background-size:0.39063rem;vertical-align:middle;margin-right:0.15625rem;}
.agent-top dl{margin-top:1.48438rem;overflow:hidden;padding:0 0.4375rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
.agent-top dl dt{float:left;margin-right:0.3125rem}
.agent-top dl dt img{border:0.07813rem solid #AFC0DC;width:1.85938rem;height:1.85938rem;-webkit-border-radius:50%;-moz-border-radius:50%;border-radius:50%;margin:0 auto;}
.agent-top dl dd{float:left}
.agent-top dl dd h1{font-size:0.5625rem;color:#fff;height:0.70313rem;width:4.375rem;overflow:hidden;margin-top:0.23438rem;font-weight:normal}
.agent-top dl dd p{font-size:0.375rem;color:#fff;margin-top:0.15625rem;}
.agent-top dl dd.dd-two{float:right;margin-top:0.3125rem}
.agent-top dl dd.dd-two a{color:#FFFF66;font-size:0.375rem;}
.agent-header ul{overflow:hidden;width:100%;background:#00297A;height:1.5625rem;border-top:1px solid #667AA3}
.agent-header ul li{float:left;width:33.3%;text-align:center;color:#fff;border-left:1px solid #667AA3;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;height:1.5625rem;padding-top:0.15625rem;font-size:0.375rem}
.agent-header ul li:nth-child(1){border-left:0}
.agent-header ul li b{display:block;font-size:0.5625rem;font-family:Arial, Helvetica, sans-serif;}

.agent-cont{width:100%;background:#fff;border-bottom:1px solid #E5E5E5;margin-top:0.15625rem}
.agent-cont ul li{overflow:hidden;border-top:1px solid #E5E5E5}
.agent-cont ul li span{float:left;width:2.03125rem;height:1.39063rem;line-height:1.39063rem;background:url(../images/line.jpg) right center no-repeat;background-size:1px 0.75rem;}
.agent-cont ul li a{float:right;height:1.39063rem;line-height:1.39063rem;font-size:0.4375rem;color:#000;width:7.96875rem;background:url(../images/icon008.jpg) 7.1875rem center no-repeat;background-size:0.3125rem;padding-left:0.67188rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
.agent-cont ul li span i{display:inline-block;width:100%;height:100%;}
.agent-cont ul li:nth-child(1) span i{background:url(../images/icon002.png) center center no-repeat;background-size:0.76563rem;}
.agent-cont ul li:nth-child(2) span i{background:url(../images/icon003.png) center center no-repeat;background-size:0.75rem;}
.agent-cont ul li:nth-child(3) span i{background:url(../images/icon004.png) center center no-repeat;background-size:0.5625rem;}
.agent-cont ul li:nth-child(4) span i{background:url(../images/icon005.png) center center no-repeat;background-size:0.5625rem;}

.agent-footer{height:1.25rem;background:#003399;position:fixed;bottom:0;width:10rem}
.agent-footer ul{overflow:hidden}
.agent-footer li{width:50%;float:left;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
.agent-footer li a{font-size:0.4375rem;color:#fff;display:block;width:100%;padding-left:2.03125rem;line-height:1.25rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
.agent-footer li:nth-child(1) a{border-right:1px solid #335CAD;background:url(../images/icon007.png) 0.85938rem center no-repeat;background-size:0.6875rem;}
.agent-footer li:nth-child(2) a{border-right:1px solid #335CAD;background:url(../images/icon006.png) 0.85938rem center no-repeat;background-size:0.6875rem;}


/* 代理商管理-充值 */
.agent-recharge{padding-bottom:1.5625rem;}
.agent-public-header{height:1.40625rem;background:url(../images/bg2.jpg) no-repeat;background-size:10rem;position:relative}
.agent-public-header a.return{display:inline-block;width:0.3125rem;height:0.5rem;background:url(../images/icon001.png) no-repeat;background-size:0.3125rem;margin-top:0.46875rem;margin-left:0.3125rem;}
.agent-public-header .headline{font-size:0.5625rem;color:#fff;position:absolute;width:6.25rem;left:0;right:0;margin:0 auto;height:1.40625rem;line-height:1.40625rem;text-align:center;top:0}
.agent-public-header a.ctrul{font-size:0.375rem;color:#fff;float:right;margin-right:0.3125rem;display:inline;height:1.40625rem;line-height:1.40625rem;}

.agent-recharge-top{margin-top:0.625rem;}
.agent-recharge-top dt img{width:1.70313rem;height:1.70313rem;-webkit-border-radius:50%;-moz-border-radius:50%;border-radius:50%;margin:0 auto}
.agent-recharge-top dd{text-align:center;font-size:0.4375rem;margin:0.3125rem auto}

.agent-recharge form{width:8.75rem;margin:0 auto}
.agent-recharge form .amount{border:1px solid #E2E2E2;height:1.40625rem;background:#fff;margin-bottom:0.3125rem;-webkit-border-radius:0.07813rem;-moz-border-radius:0.07813rem;border-radius:0.07813rem;font-size:0.4375rem;color:#CCC}
.agent-recharge form .amount input{display:inline-block;width:7.89063rem;height:1.375rem;line-height:1.375rem;font-size:0.4375rem;color:#CCc;padding:0 0.3125rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;-webkit-border-radius:0.07813rem;-moz-border-radius:0.07813rem;border-radius:0.07813rem;color:#666}
.agent-recharge form input.btn{display:block;width:8.75rem;height:1.40625rem;line-height:1.40625rem;background:#00349A;font-size:0.5625rem;text-align:center;color:#fff;-webkit-border-radius:0.07813rem;-moz-border-radius:0.07813rem;border-radius:0.07813rem;margin-top:0.625rem;}

.agent-recharge-recoder .title{border-bottom:1px solid #999;position:relative;height:0.46875rem;margin:0.78125rem auto}
.agent-recharge-recoder .title span{display:inline-block;font-size:0.4375rem;color:#666;background:#F0F0F0;position:absolute;left:0;right:0;margin:0 auto;width:3.90625rem;bottom:-0.23438rem;text-align:center}
.agent-recharge-recoder dl{width:8.75rem;margin:0.3125rem auto;overflow:hidden}
.agent-recharge-recoder dl dt img{width:1.29688rem;height:1.29688rem;-webkit-border-radius:0.15625rem;-moz-border-radius:0.15625rem;border-radius:0.15625rem}
.agent-recharge-recoder dl dt{float:left}
.agent-recharge-recoder dl dd{width:6.875rem;float:right}
.agent-recharge-recoder dl dd ul li{font-size:0.375rem;line-height:0.54688rem;color:#656565}
.agent-recharge-recoder dl dd ul li i{font-style:normal;color:#CC0000}

/* 代理商管理-成员列表 */
.agent-list{padding-bottom:1.5625rem}
.agent-list-search{overflow:hidden;border:1px solid #CCC;-webkit-border-radius:0.07813rem;-moz-border-radius:0.07813rem;border-radius:0.07813rem;width:9.21875rem;margin:0.46875rem auto;background:#fff;height:1.03125rem;}
.agent-list-search .text{float:left;width:8.04688rem;height:1.03125rem;line-height:1.03125rem;font-size:0.375rem;padding:0 0.3125rem 0 0.46875rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
.agent-list-search .btn{float:right;width:1.09375rem;height:1.03125rem;background:#fff url(../images/icon011.png) left center no-repeat;background-size:0.75rem;cursor:pointer}
.agent-list-search input::-moz-placeholder{color:#CCC;}   
.agent-list-search input::-ms-input-placeholder{color:#CCC;}   
.agent-list-search input::-webkit-input-placeholder{color:#CCC;}  
.agent-list-cont dl{overflow:hidden;background:#fff;border:solid #E5E5E5;border-width:1px 0 1px 0;margin-bottom:0.14063rem;padding:0.3125rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
.agent-list-cont dl dt,.agent-list-cont dl dd{float:left}
.agent-list-cont dl dt img{width:1.48438rem;height:1.48438rem;-webkit-border-radius:50%;-moz-border-radius:50%;border-radius:50%;}
.agent-list-cont dl .dd-one{margin-left:0.3125rem;display:inline;width:4.6875rem;}
.agent-list-cont dl .dd-one h1{font-size:0.4375rem;color:#333;font-weight:normal;height:0.625rem;width:5.46875rem;overflow:hidden}
.agent-list-cont dl .dd-one ul li{font-size:0.375rem;color:#999;line-height:0.54688rem;}
.agent-list-cont dl .dd-two{float:right;margin-top:20px;}
.agent-list-cont dl .dd-two a{display:inline-block;color:#fff;background:#C90000;-webkit-border-radius:0.04688rem;-moz-border-radius:0.04688rem;border-radius:0.04688rem;text-align:center;padding:0.15625rem 0.3125rem;margin-top:0.15625rem;font-size:0.375rem;}
.agent-list-cont dl .dd-two a:nth-child(1){background:#EBB201}

/* 代理商管理-订单 */
.agent-indent-tab{overflow:hidden;background:#fff}
.agent-indent-crt{color:#C90000 !important}
.agent-indent-tab li{float:left;width:33.3%;text-align:center;font-size:0.375rem;color:#333;height:1.39063rem;line-height:1.39063rem;border-right:1px solid #CCC;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;cursor:pointer}
.agent-indent{padding-bottom:1.5625rem}
.agent-indent-tab li:nth-child(3){border-right:0}
.agent-indent-cont dl{overflow:hidden;background:#fff;border:solid #E5E5E5;border-width:1px 0 1px 0;margin-bottom:0.14063rem;padding:0.39063rem 0.3125rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
.agent-indent-cont dl dt,.agent-indent-cont dl dd{float:left}
.agent-indent-cont dl dt img{width:1.48438rem;height:1.48438rem;-webkit-border-radius:50%;-moz-border-radius:50%;border-radius:50%;}
.agent-indent-cont dl .dd-one{margin-left:0.3125rem;display:inline;width:5.569rem;}
.agent-indent-cont dl .dd-one ul li{font-size:0.375rem;color:#666;line-height:0.625rem;}
.agent-indent-cont dl .dd-one ul li span{color:#D02626}
.agent-indent-cont dl .dd-one ul li .green{color:#339933}
.agent-indent-cont dl .dd-two{float:right}
.agent-indent-cont dl .dd-two a{display:block;color:#fff;background:#C90000;-webkit-border-radius:0.04688rem;-moz-border-radius:0.04688rem;border-radius:0.04688rem;text-align:center;padding:0.125rem 0.15625rem;margin-top:0.15625rem;font-size:0.375rem;}
.agent-indent-cont dl .dd-two a.gray{background:#999}

/* 代理商管理-添加成员 */
.agent-add form{padding-top:0.3125rem}
.agent-add form p{background:#fff;border:solid #E5E5E5;border-width:1px 0 1px 0;margin-bottom:0.14063rem;height:1.25rem;line-height:1.25rem;padding:0 0.3125rem 0 0.78125rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;overflow:hidden}
.agent-add form p label{font-size:0.4375rem;color:#333;float:left}
.agent-add form p input{font-size:0.375rem;color:#333;float:left;width:5.46875rem;height:1.25rem;line-height:1.25rem;}
.agent-add form p input::-moz-placeholder{color:#999;}   
.agent-add form p input::-ms-input-placeholder{color:#999;}   
.agent-add form p input::-webkit-input-placeholder{color:#999;}  
.agent-add form input.btn{display:block;font-size:0.4375rem;color:#fff;text-align:center;background:#FF9900;width:2.57813rem;height:0.78125rem;margin:0.78125rem auto;-webkit-border-radius:0.04688rem;-moz-border-radius:0.04688rem;border-radius:0.04688rem;cursor:pointer}

.upload2{float:right;display:block; width:5.46875rem;height: 0.5rem;position: relative; text-decoration: none;margin-top:0.3125rem;height: 0.5rem;line-height:0.5rem;color:#666;text-align:right;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;font-size:0.375rem;}
.change2{position: absolute; overflow: hidden; right: 0;top: 0;opacity: 0;width:5.46875rem;}

/* 代理商管理-详情 */
.agent-show{padding-top:0.3125rem}
.agent-show p{background:#fff;border:solid #E5E5E5;border-width:1px 0 1px 0;margin-bottom:0.14063rem;height:1.25rem;line-height:1.25rem;padding:0 0.3125rem 0 0.78125rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;overflow:hidden}
.agent-show p span{font-size:0.4375rem;color:#333;float:left}
.agent-show p i{font-size:0.375rem;color:#999;float:left;width:5.46875rem;height:1.25rem;line-height:1.25rem;font-style:normal}


/* 代理商管理-我的彩虹币 */
.agent-rainbow{padding-bottom:1.5625rem;}

.agent-rainbow-top{margin-top:0.625rem;}
.agent-rainbow-top dt img{width:1.76563rem;height:1.76563rem;margin:0 auto}
.agent-rainbow-top dd.dd-one{font-size:0.375rem;color:#666;text-align:center;margin-top:0.15625rem;}
.agent-rainbow-top dd.dd-two{text-align:center;font-size:0.75rem;margin:0.28125rem auto;color:#CC0000;font-weight:bold}
.agent-rainbow form{width:8.75rem;margin:0.625rem auto}
.agent-rainbow form .amount{border:1px solid #E2E2E2;height:1.40625rem;background:#fff;margin-bottom:0.3125rem;-webkit-border-radius:0.07813rem;-moz-border-radius:0.07813rem;border-radius:0.07813rem;font-size:0.4375rem;color:#CCC}
.agent-rainbow form .amount input{display:inline-block;width:7.89063rem;height:1.375rem;line-height:1.375rem;font-size:0.4375rem;color:#CCc;padding:0 0.3125rem;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;-webkit-border-radius:0.07813rem;-moz-border-radius:0.07813rem;border-radius:0.07813rem;color:#666}
.agent-rainbow form input.btn{display:block;width:8.75rem;height:1.40625rem;line-height:1.40625rem;background:#FF9900;font-size:0.5625rem;text-align:center;color:#fff;-webkit-border-radius:0.07813rem;-moz-border-radius:0.07813rem;border-radius:0.07813rem;margin-top:0.625rem;border:1px solid #E2E2E2}

.agent-rainbow-recoder .title{border-bottom:1px solid #DD4B45;position:relative;height:0.46875rem;margin:0.9375rem auto}
.agent-rainbow-recoder .title span{display:inline-block;font-size:0.4375rem;color:#DD4B45;background:#F0F0F0;position:absolute;left:0;right:0;margin:0 auto;width:3.90625rem;bottom:-0.23438rem;text-align:center}
.agent-rainbow-recoder dl{padding:0 0.3125rem;overflow:hidden;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;margin-bottom:0.625rem;}
.agent-rainbow-recoder dl dt img{width:1.29688rem;height:1.29688rem;-webkit-border-radius:0.15625rem;-moz-border-radius:0.15625rem;border-radius:0.15625rem}
.agent-rainbow-recoder dl dt{float:left}
.agent-rainbow-recoder dl dd{float:left}
.agent-rainbow-recoder dl dd.dd-two{float:right}
.agent-rainbow-recoder dl dd.dd-one{display:inline;margin-left:0.3125rem}
.agent-rainbow-recoder dl dd ul li{font-size:0.375rem;line-height:0.46875rem;color:#333}
.agent-rainbow-recoder dl dd ul li span{color:#009933}
.agent-rainbow-recoder dl dd ul li span.red{color:#FF0000}
.agent-rainbow-recoder dl dd.dd-two a{display:block;color:#fff;background:#DD4B45;-webkit-border-radius:0.04688rem;-moz-border-radius:0.04688rem;border-radius:0.04688rem;text-align:center;padding:0.04688rem 0.15625rem;margin-top:0.15625rem;font-size:0.375rem;}
.agent-rainbow-recoder dl dd.dd-two a.gray{background:#999}

.rainbow-popup{width:7.96875rem;background:#fff;position:fixed;left:0;right:0;margin:0 auto;top:3.28125rem;-webkit-border-radius:0.07813rem;-moz-border-radius:0.07813rem;border-radius:0.07813rem;border:1px solid #ccc;padding-top:0.78125rem;padding-bottom:0.5rem;display:none}

.rainbow-popup ul li{font-size:0.34375rem;color:#666;line-height:0.75rem;text-align:center}
.rainbow-popup ul li span{font-size:0.46875rem;color:#999}
.rainbow-popup ul li b{font-size:0.75rem;color:#DD4B45;display:block;margin:0.3125rem auto}
.rainbow-popup input{display:block;height:0.85938rem;line-height:0.85938rem;-webkit-border-radius:0.07813rem;-moz-border-radius:0.07813rem;border-radius:0.07813rem;border:1px solid #CCC;width:2.8125rem;text-align:center;margin:0 auto;font-size:0.375rem;color:#fff;background:#DD4B45;margin-top:0.28125rem;}

/* 代理商管理-代理商信息 */
.agent-infor form{padding:0.3125rem 0 1.5625rem 0}
